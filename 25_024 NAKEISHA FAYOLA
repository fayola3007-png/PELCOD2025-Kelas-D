#tugas ke-1
nama = "Nakeisha fayola jasmine wibisono"
umur = 18
tinggi_badan = 162.0
angka_favorit = 7

print("nama:", nama)
print("umur:", umur)
print("tinggi_badan:", tinggi_badan)
print("angka_favorit", angka_favorit)

#tugas ke-2
harga_pensil = 2000
jumlah_pensil = 4
harga_buku = 5000
jumlah_buku = 2
print(harga_pensil * jumlah_pensil + harga_buku * jumlah_buku)

#tugas ke-3
if angka_favorit % 2 == 0 :
    print("angka genap")
else:
    print("angka ganjil")
    
